{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "standard"
  ],
  "globals": {
    "fetch": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-console": 0,
    "space-before-function-paren": [
      "error",
      "never"
    ]
  }
}